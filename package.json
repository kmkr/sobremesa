{
  "name": "sobremesa",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack -p",
    "build:css": "postcss -o dist/main.css src/main.css",
    "watch:css": "nodemon -e css --watch src/main.css --exec npm run build:css",
    "postinstall": "npm run build",
    "start:client": "NODE_ENV=development webpack -w",
    "start:server": "nodemon -e css,js --watch dist/main.css --watch lib lib/index.js",
    "start": "node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmkr/sobremesa.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kmkr/sobremesa/issues"
  },
  "homepage": "https://github.com/kmkr/sobremesa#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "nodemon": "^1.18.9",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.11",
    "postcss-cli": "^6.1.1",
    "postcss-css-variables": "^0.11.0",
    "postcss-import": "^12.0.1",
    "preact": "^8.4.2",
    "prettier": "1.15.3",
    "throttle-debounce": "^2.1.0",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "md5-file": "^4.0.0",
    "mongodb": "^3.1.10",
    "pug": "^2.0.3"
  }
}
